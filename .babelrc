{
  "env": {
    "testing": {
      "presets": ["react", "env"],
      "plugins": ["transform-runtime"]
    },
    "production": {
      "presets": [
        "react",
        ["env", {
          "modules": false
        }]
      ],
      "plugins": ["external-helpers", "transform-export-default"]
    }
  }
}
