{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/react": "^3.4.10",
    "chroma-js": "^1.3.7",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
    "storybook": "yarn tailwind:css && start-storybook -p 9001 -c .storybook",
    "start": "yarn tailwind:css && react-scripts start",
    "build": "yarn tailwind:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "eact-scripts eject"
  },
  "devDependencies": {
    "@buildit/storybook-addon-versions": "^0.4.3",
    "@storybook/addon-a11y": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "tailwindcss": "^0.6.5"
  }
}
